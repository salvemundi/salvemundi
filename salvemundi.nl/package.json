{
  "name": "salvemundi.nl",
  "version": "1.0.7-beta",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "test": "npm run test:components && npm run test:e2e",
    "test:components": "./node_modules/jest/bin/jest.js ",
    "test:e2e": "VUE_APP_MODE=test npm run build && npm run test:e2e-quick",
    "test:e2e-quick": "./node_modules/.bin/testcafe 'chrome:headless --disable-setuid-sandbox --window-size=1920x1080 --no-sandbox' ./e2e/**/*.test.ts --app \"node_modules/http-server/bin/http-server ./dist --proxy \\\"http://localhost:8080?\\\" -c-1 -s\""
  },
  "dependencies": {
    "@types/dotenv": "^6.1.1",
    "@types/dotenv-webpack": "^1.7.0",
    "@types/jest": "^24.0.23",
    "@types/node": "^12.12.12",
    "axios": "^0.19.0",
    "bootstrap": "^4.3.1",
    "bootstrap-vue": "^2.1.0",
    "core-js": "^2.6.10",
    "dotenv": "^8.2.0",
    "dotenv-webpack": "^1.7.0",
    "inversify": "^5.0.1",
    "moment": "^2.24.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^6.5.3",
    "vue": "^2.6.10",
    "vue-class-component": "^7.0.2",
    "vue-cookies": "^1.5.13",
    "vue-i18n": "^8.15.0",
    "vue-localstorage": "^0.6.2",
    "vue-property-decorator": "^8.3.0",
    "vue-router": "^3.1.3",
    "vue-toasted": "^1.1.27",
    "vuex": "^3.1.2",
    "whatwg-fetch": "^3.0.0"
  },
  "devDependencies": {
    "@kazupon/vue-i18n-loader": "^0.4.1",
    "@types/vue-i18n": "^7.0.0",
    "@vue/cli-plugin-babel": "^3.12.1",
    "@vue/cli-plugin-typescript": "^3.12.1",
    "@vue/cli-service": "^3.12.1",
    "@vue/test-utils": "^1.0.0-beta.29",
    "babel-jest": "23.6.0",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-stage-3": "^6.24.1",
    "http-server": "^0.11.1",
    "jest": "23.6.0",
    "jest-serializer-vue": "^2.0.2",
    "node-sass": "^4.13.0",
    "sass-loader": "^7.3.1",
    "testcafe": "^1.7.0",
    "ts-jest": "23.10.5",
    "typescript": "^3.7.2",
    "vue-jest": "4.0.0-beta.2",
    "vue-template-compiler": "^2.6.10"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts",
      "vue"
    ],
    "transform": {
      "^.+\\.ts$": "ts-jest",
      ".*\\.(vue)$": "vue-jest"
    },
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/src/$1"
    },
    "snapshotSerializers": [
      "jest-serializer-vue"
    ],
    "testRegex": "(/__tests__/.*|(\\.|/)(spec))\\.(jsx?|tsx?)$"
  }
}
